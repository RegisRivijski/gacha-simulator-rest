<%- include('./blocks/firstName', { $t, userData }); %>

<% if (canBuy) { %>
    , <%- $t('wish10.dropped'); %><br>

    <% for (const wishData of wishesData) { %>
        <a href="<%= wishData.newItem.newItemData?.url?.fandom ?? ''; %>">
            <%= '⭐️'.repeat(wishData.newItem.newItemData.rarity); %> <b><%= wishData.newItem.newItemData.name; %></b>
        </a>

        <% if (wishData.cashBackForDuplicate.cashBackTemplate) { %>
            , +<%= wishData.cashBackForDuplicate.cashBackTemplate; %>
        <% } %>
        <br>
    <% } %>

    <br>

    🔸<i><%= bannerName; %></i>

    <br>

    <%- include('./blocks/spendCurrency', { $t, templatePrices }); %>
<% } else { %>
    , <%- $t(`wish10.noPrimogems.${currentBannerType}`)%>
<% } %>
