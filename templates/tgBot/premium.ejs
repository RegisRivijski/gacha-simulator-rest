<% if (additionalData.isPremium) { %>
    🎉 <b><%- $t('premium.active.title'); %></b> 🎉<br>
    <br>
    <%- include('./blocks/premiumRealTimeExample', { $t, additionalData }); %>
    <br>
    ⏳ <%- $t('premium.active.remainingDays').replace('{{days}}', additionalData.premiumDays); %>.<br>
    💎 <%- $t('premium.active.dailyReminder'); %><br>
    <br>
    💡 <i><%- $t('premium.active.enjoy'); %></i>
<% } else { %>
    🎉 <b><%- $t('premium.welcome'); %></b> 🎉<br>
    <br>
    🧐 <b><%- $t('premium.benefits'); %></b><br>
    🔸 <%- $t('premium.benefit.speed'); %><br>
    🔸 <%- $t('premium.benefit.limit').replace('{{valuePremium}}', economy.PRIMOGEMS_GET_MAX_PREMIUM).replace('{{valueDefault}}', economy.PRIMOGEMS_GET_MAX); %><br>
    🔸 <%- $t('premium.benefit.dailyBonus'); %>: <b>+<%= economy.PRIMOGEMS_DAILY_PREMIUM; %></b> ✦<br>
    <br>
    💰 <b><%= $t('premium.invoice.itemPrice'); %></b><br>
    🔸 <b><%= $t('premium.cost.month'); %></b>: <%= economy.PREMIUM_TYPE_MONTH_COST %> ⭐️(<s><%= $t('premium.oldPrice'); %> <%= economy.PREMIUM_TYPE_MONTH_COST_OLD %> ⭐️</s>)<br>
    🔸 <b><%= $t('premium.cost.forever'); %></b>: <%= economy.PREMIUM_TYPE_FOREVER_COST %> ⭐️(<s><%= $t('premium.oldPrice'); %> <%= economy.PREMIUM_TYPE_FOREVER_COST_OLD %> ⭐️</s>)<br>
    <br>
    <%- include('./blocks/premiumRealTimeExample', { $t, additionalData }); %>
    <br>
    💡 <i><%- $t('premium.motivation'); %></i>
<% } %>
