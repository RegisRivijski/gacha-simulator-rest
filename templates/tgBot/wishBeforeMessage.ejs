<%- include('./blocks/firstName', { $t, userData }); %>

<%- $t('wish.spins') %>:

<br>
<br>

🔸 <i><%= currentBannerData.bannersName %></i> <%= currentBannerData.bannerEmoji %>

<% if (
        currentBannerData?.items.length
        && currentBannerData.category === EVENT_BANNER_CATEGORY_NAME
        && currentBannerData.type !== STANDARD_BANNER_TYPE_NAME
) { %>
        <% currentBannerData.items.forEach((itemData) => { %>
            <% if (Number(itemData.rarity) === 5) { %>
                <br>
                · 5⭐️ <%= itemData.name %> ↑
            <% } %>
        <% }); %>
<% } %>
